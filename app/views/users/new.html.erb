<div class='container'>
	<div class='row'>
		<h2>Register</h2>
		<p id="notice"><%= notice %></p>
		<%= simple_form_for @user do |f| %>
		<%= f.input :first_name %>
		<%= f.input :last_name %>
		<%= f.input :email %>
		<%= f.input :password %>
		<%= f.input :password_confirmation %>
		<%= f.submit "Create user" %>
		<% end %>
	</div>
	<div class='row'>
		<p>Already a user?<%= link_to("Login", signin_path) %> here</p>
	</div>
</div>
