<div class='container'>
	<div class='row'>
		<h2>Login</h2>
		<p id="notice"><%= notice %></p>
		<%= simple_form_for(:session, :url => sessions_path) do |f| %>
		<%= f.input :email %>
		<%= f.input :password %>
		<%= f.submit "Login" %>
		<% end %>
	</div>
	<div class='row'>
		<p>Not a user yet?<%= link_to("Register", signup_path) %> here!</p>
	</div>
</div>